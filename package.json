    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "odata-sequelize": "^1.0.21",
    "parcel": "^2.0.0-beta.3.1",
    "pug": "^3.0.2",
    "sequelize": "^6.6.2",
    "socket.io": "^4.0.1",
    "sqlite3": "^5.0.2",
    "url-slug": "^3.0.1",
