{
  "name": "darts",
  "version": "1.0.0",
  "description": "a dartboard",
  "main": "server.js",
  "client": "./public/js/bundle.js",
  "targets": {
    "main": false,
    "client": {}
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --ext .js,.json,.pug,.env",
