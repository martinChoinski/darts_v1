{
  "name": "darts",
  "version": "1.0.0",
  "description": "its the meat between darts device and unity",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --ext .js,.json,.pug,.env",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "watch:js": "parcel watch ./public/js/index.js --no-source-maps --out-dir ./public/js/ --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --no-source-maps --out-dir ./public/js/ --out-file bundle.js",
    "help:parcel": "parcel help build"
  },
  "author": "mc",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "decode-uri-component": "^0.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "odata-sequelize": "^1.0.21",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "sequelize": "^6.6.2",
    "socket.io": "^4.0.1",
    "sqlite3": "^5.0.2",
    "url-slug": "^3.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
}
